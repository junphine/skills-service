{
  "name": "@skilltree/skills-client-display",
  "main": "./dist/userSkills.common.js",
  "version": "1.0.0",
  "license": "Apache-2.0",
  "description": "SkillTree Client Display UI",
  "author": "SkillTree Team",
  "private": "true",
  "scripts": {
    "preinstall": "npm run setup:common",
    "serve": "npm run setup:common && vue-cli-service serve",
    "licenseDepCheck": "license-checker --excludePrivatePackages --production --onlyAllow='MIT;ISC;Apache-2.0;BSD-2-Clause;Custom: https://travis-ci.org/component/emitter.png' --summary",
    "licenseHeaderCheck": "license-check-and-add check -f '../license-add/license-add-config.json' -r 20",
    "licenseHeaderAdd": "license-check-and-add add -f ../license-add/license-add-config.json -r 21",
    "licenseCheck": "npm run licenseHeaderCheck && npm run licenseDepCheck",
    "build": "npm run setup:common && vue-cli-service build --no-module",
    "build-lib": "NODE_ENV='production' && rm -rfv dist && npm run setup:common && vue-cli-service build --target lib --name userSkills './src/index.js'",
    "deploy": "mkdir -p ../service/src/main/resources/public/static/ && mkdir -p ../service/target/classes/public/static && rm -rf ../service/src/main/resources/public/static/clientPortal && cp -rT dist ../service/src/main/resources/public/static/clientPortal && cp -rT dist ../service/target/classes/public/static/clientPortal ",
    "lint": "vue-cli-service lint",
    "test:unit": "vue-cli-service test:unit",
    "setup:common": "npx symlink-dir ../common-components/src/common src/common-components"
  },
  "dependencies": {
    "@fortawesome/fontawesome-free": "5.15.4",
    "@toast-ui/editor": "3.2.1",
    "@toast-ui/vue-editor": "3.2.2",
    "@vue-a11y/announcer": "2.1.0",
    "animate.css": "4.1.1",
    "apexcharts": "3.29.0",
    "axios": "0.23.0",
    "axios-auth-refresh": "3.2.1",
    "bootstrap": "4.5.3",
    "bootstrap-vue": "2.21.2",
    "dayjs": "1.10.7",
    "lodash": "4.17.21",
    "lodash.debounce": "4.0.8",
    "material-icons": "1.9.1",
    "node-emoji": "1.11.0",
    "number-format.js": "2.0.9",
    "postmate": "1.5.2",
    "q": "1.5.1",
    "tinycolor2": "1.4.2",
    "tui-editor-plugin-font-size": "1.0.4",
    "url-search-params-polyfill": "8.1.1",
    "vee-validate": "3.4.14",
    "video.js": "8.3.0",
    "vis-data": "7.1.6",
    "vis-network": "9.1.6",
    "vue": "2.6.14",
    "vue-apexcharts": "1.6.2",
    "vue-js-toggle-button": "1.3.3",
    "vue-radial-progress": "0.3.2",
    "vue-router": "3.5.2",
    "vue-select": "3.20.2",
    "vue-simple-progress": "1.1.1",
    "vue-simple-spinner": "1.2.10",
    "vuex": "3.6.2"
  },
  "devDependencies": {
    "@babel/core": "7.15.8",    
    "@vue/cli-plugin-babel": "5.0.8",
    "@vue/cli-plugin-eslint": "5.0.8",
    "@vue/cli-plugin-unit-jest": "5.0.8",
    "@vue/cli-service": "5.0.8",
    "@vue/eslint-config-airbnb": "5.3.0",
    "@vue/test-utils": "1.2.2",
    "@babel/eslint-parser": "7.22.10",
    "babel-jest": "27.3.1",
    "error-stack-parser": "2.0.7",
    "eslint": "7.32.0",
    "eslint-plugin-vue": "9.17.0",
    "license-check-and-add": "4.0.2",
    "license-checker": "25.0.1",
    "sass": "1.49.9",
    "sass-loader": "10.1.1",
    "symlink-dir": "5.1.1",
    "vue-template-compiler": "2.6.14"
  },
  "peerDependencies": {
    "font-awesome": "4.7.0"
  },
  "files": [
    "dist/*",
    "src/*",
    "*.json",
    "*.js"
  ]
}
